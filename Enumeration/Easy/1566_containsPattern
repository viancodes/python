from typing import List

class Solution:
    def containsPattern(self, arr: List[int], m: int, k: int) -> bool:
        n = len(arr)
        cnt = 0
        for i in range(n - m):
            if arr[i] == arr[i + m]:
                cnt += 1
                if cnt >= m * (k - 1):
                    return True
            else:
                cnt = 0
        return False
